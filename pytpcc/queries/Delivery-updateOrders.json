{
  "operators": {
    "load": {
       "type": "TableLoad",
       "table": "ORDERS"
    },
    "select": {
       "type": "SimpleTableScan",
       "predicates": [
         {"type": "AND"},
         {"type": "EQ", "in": 0, "f": "O_ID", "vtype": 0 /*Int*/, "value": %(no_o_id)d},
         {"type": "AND"},
         {"type": "EQ", "in": 0, "f": "O_D_ID", "vtype": 0 /*Int*/, "value": %(d_id)d},
         {"type": "EQ", "in": 0, "f": "O_W_ID", "vtype": 0 /*Int*/, "value": %(w_id)d}
       ]
    },
    "validate" : {
      "type" : "ValidatePositions"
    },
    "update": {
       "type": "PosUpdateScan",
       "data": {
          "O_CARRIER_ID": %(o_carrier_id)d
       }
    },
    "NoOp": {
      "type": "NoOp"
    }
  },
  "edges": [["load", "select"], ["select", "validate"], ["validate", "update"], ["update","NoOp"]]
}
